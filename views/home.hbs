	<!-- One -->
	<section id="one" class="wrapper post bg-img" data-bg="banner2.jpg">
		<div class="inner">
			<form>
				<div class="mb-3">
					<h3>Iniciar sesión</h3>
					<label for="exampleInputEmail1" class="form-label">Email address</label>
					<input type="email" class="form-control" id="emaillogin" aria-describedby="emailHelp">
				</div>
				<div class="mb-3">
					<label for="exampleInputPassword1" class="form-label">Password</label>
					<input type="password" class="form-control" id="password">
				</div>

				<button id="buttonLogin" type="submit" class="btn btn-primary">Ingresar</button>
			</form>

		</div>
		<a href="#two" class="more">Learn More</a>
	</section>

	<!--  """""""""""""""""""""""""""""""""""""""""Pasajes""""""""""""""""""""""""""""""""""""""""""""""""""" -->
	<section id="two" class="wrapper post bg-img" data-bg="banner5.jpg">
		<div class="inner">
			<article class="box">
				<header>
					<h2>Pasajes</h2>
				</header>
				<form id="formulario">
					<div class="ida-vuelta pasaje">
						<div class="form-check form-check-inline">
							<input class="form-check-input" type="radio" name="idavueltaPasaje" id="idaYvueltaPasaje"
								checked>
							<label class="form-check-label" for="idaYvueltaPasaje">
								Ida y vuelta
							</label>
						</div>
						<div class="form-check form-check-inline">
							<input class="form-check-input" type="radio" name="idavueltaPasaje" id="soloIdaPasaje">
							<label class="form-check-label" for="soloIdaPasaje">
								Solo ida
							</label>
						</div>
						
					</div>
					<div class="datos-viaje">
						<div class="item" id="item1">
							<select id="selectOrigenPasaje" class="form-select inline "
								aria-label="Default select example">
								<option selected>Elegí origen</option>
								<option id="value1" value="Tigre">Tigre</option>
								<option id="value2" value="Puerto Madero">Puerto Madero</option>
							</select>
						</div>
						<div class="item" id="item2">
							<select id="selectDestinoPasaje" class="form-select " aria-label="Default select example">
								<option selected>Elegí destino</option>
								<option id="value1" value="Montevideo">Montevideo</option>
								<option id="value2" value="Punta Del Este">Punta Del Este </option>
							</select>
						</div>

						<div class="item" id="item3">
							<input id="SalidaPasaje" class="form-control" type="date" />
						</div>

						<div class="item" id="item4">
							<input id="vueltaPasaje" class="form-control" type="date" />
						</div>

					</div>
					<div class="contenedor-acompañante contenedor-acompañante-pasaje">
						<div class="contenedor-menores contenedor-menores-pasaje">
							<div class="contenedor-adulto-basico contenedor-adulto-basico-pasaje">
								<span>Menores Basico </span><input type="number" min="0">
							</div>
							<div class="contenedor-adulto-medio contenedor-adulto-medio-pasaje">
								<span>Menores Medium </span><input type="number" min="0">
							</div>
							<div class="contenedor-adulto-premium contenedor-adulto-premium-pasaje">
								<span>Menores Premium </span> <input type="number" min="0">
							</div>
						</div>
						<div class="contenedor-mayores contenedor-mayores-pasaje">
							<div class="contenedor-adulto-basico contenedor-adulto-basico-pasaje">
								<span>Adultos Basico </span><input type="number" min="0">
							</div>
							<div class="contenedor-adulto-medio contenedor-adulto-medio-pasaje">
								<span>Adultos Medium </span><input type="number" min="0">
							</div>
							<div class="contenedor-adulto-premium contenedor-adulto-premium-pasaje">
								<span>Adultos Premium </span> <input type="number" min="0">
							</div>
						</div>
					</div>

					<!-- aca -->
					<div class="botones">
						<button id="botonBuscarPasaje" type="button" class="btn btn-primary enabled">Buscar</button>
						<button id="botonVolverAbuscarPasaje" type="button" class="btn btn-primary disabled">volver a
							buscar</button>
					</div>

				</form>

				<div id="resultadoPasaje">

				</div>

			</article>
		</div>
		<a href="#four" class="more">Learn More</a>
	</section>

